{
  "name": "akila-analytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start --verbose",
    "staging-build": "REACT_APP_STAGE=staging craco build",
    "prod-build": "REACT_APP_STAGE=prod craco build",
    "test": "react-scripts test -- --config jest.config.js --coverage --testResultsProcessor=\"jest-junit\"",
    "eject": "craco eject",
    "deploy": "aws s3 sync build/ s3://www.akilacap.com"
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "craco-alias": "3.0.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest-junit": "^14.0.0",
    "prettier": "^2.6.2",
    "webpack-cli": "4.9.2"
  },
  "dependencies": {
    "@craco/craco": "6.4.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.3.1",
    "@mui/material": "5.6.0",
    "@mui/styles": "5.3.0",
    "@stripe/react-stripe-js": "^1.7.1",
    "@stripe/stripe-js": "^1.27.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/user-event": "13.5.0",
    "amazon-cognito-identity-js": "5.2.8",
    "axios": "^0.27.2",
    "craco": "0.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "package.json": "2.0.1",
    "prop-types": "^15.8.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "react-slick": "0.28.1",
    "sass": "1.49.7",
    "slick-carousel": "1.8.1",
    "styled-components": "^5.3.5",
    "web-vitals": "2.1.4"
  }
